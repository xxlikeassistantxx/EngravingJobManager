<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EngravingJobManager.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EngravingJobManager"
    Shell.FlyoutBehavior="Disabled"
    Title="EngravingJobManager">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent Route="CreateJobPage" ContentTemplate="{DataTemplate local:CreateJobPage}" IsVisible="False" />

</Shell>